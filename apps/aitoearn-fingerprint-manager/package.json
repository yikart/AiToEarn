{
  "name": "@aitoearn/aitoearn-fingerprint-manager",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "aitoearn-fingerprint-manager",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "--node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "--node-env=development"
            ]
          }
        }
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "local",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "aitoearn-fingerprint-manager:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "local": {
            "buildTarget": "aitoearn-fingerprint-manager:build:development",
            "args": [
              "-c",
              "apps/aitoearn-fingerprint-manager/config/local.config.js"
            ]
          },
          "dev": {
            "buildTarget": "aitoearn-fingerprint-manager:build:development",
            "args": [
              "-c",
              "apps/aitoearn-fingerprint-manager/config/dev.config.js"
            ]
          },
          "prod": {
            "buildTarget": "aitoearn-fingerprint-manager:build:production",
            "args": [
              "-c",
              "apps/aitoearn-fingerprint-manager/config/prod.config.js"
            ]
          }
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": ["{options.outputFile}"],
        "options": {
          "fix": true
        }
      }
    }
  },
  "dependencies": {
    "@aitoearn/common": "workspace:^",
    "@nestjs/common": "^11.0.0",
    "@nx/webpack": "21.3.11",
    "zod": "^4.0.15"
  }
}

{
  "name": "fingerprint-manager",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/fingerprint-manager/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/fingerprint-manager",
        "tsConfig": "apps/fingerprint-manager/tsconfig.app.json",
        "packageJson": "apps/fingerprint-manager/package.json",
        "main": "apps/fingerprint-manager/src/index.ts",
        "assets": ["apps/fingerprint-manager/*.md"],
        "generatePackageJson": true,
        "clean": true
      }
    },
    "prune-lockfile": {
      "dependsOn": ["build"],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/fingerprint-manager/package.json",
        "{workspaceRoot}/dist/apps/fingerprint-manager/pnpm-lock.yaml"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": ["build"],
      "cache": true,
      "outputs": [
        "{workspaceRoot}/dist/apps/fingerprint-manager/workspace_modules"
      ],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": ["prune-lockfile", "copy-workspace-modules"],
      "executor": "nx:noop"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "local",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "fingerprint-manager:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "local": {
          "buildTarget": "fingerprint-manager:build:development",
          "args": [
            "-c",
            "{workspaceRoot}/apps/fingerprint-manager/config/local.config.js"
          ]
        },
        "dev": {
          "buildTarget": "fingerprint-manager:build:development",
          "args": [
            "-c",
            "{workspaceRoot}/apps/fingerprint-manager/config/dev.config.js"
          ]
        },
        "prod": {
          "buildTarget": "fingerprint-manager:build:production",
          "args": [
            "-c",
            "{workspaceRoot}/apps/fingerprint-manager/config/prod.config.js"
          ]
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "fix": true
      }
    }
  }
}

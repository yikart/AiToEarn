{
  "name": "aitoearn-cloud-space",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/aitoearn-cloud-space/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/aitoearn-cloud-space",
        "tsConfig": "apps/aitoearn-cloud-space/tsconfig.app.json",
        "packageJson": "apps/aitoearn-cloud-space/package.json",
        "main": "apps/aitoearn-cloud-space/src/main.ts",
        "assets": ["apps/aitoearn-cloud-space/*.md"],
        "generatePackageJson": true,
        "clean": true
      }
    },
    "prune-lockfile": {
      "dependsOn": ["build"],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/aitoearn-cloud-space/package.json",
        "{workspaceRoot}/dist/apps/aitoearn-cloud-space/pnpm-lock.yaml"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "local",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "cloud-space:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "local": {
          "buildTarget": "aitoearn-cloud-space:build:development",
          "args": [
            "-c",
            "{workspaceRoot}/apps/aitoearn-cloud-space/config/local.config.js"
          ]
        },
        "dev": {
          "buildTarget": "aitoearn-cloud-space:build:development",
          "args": [
            "-c",
            "{workspaceRoot}/apps/aitoearn-cloud-space/config/dev.config.js"
          ]
        },
        "prod": {
          "buildTarget": "aitoearn-cloud-space:build:production",
          "args": [
            "-c",
            "{workspaceRoot}/apps/aitoearn-cloud-space/config/prod.config.js"
          ]
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "fix": true
      }
    },
    "docker-context": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "cache": true,
      "options": {
        "command": "node scripts/prepare-docker-context.js aitoearn-cloud-space",
        "cwd": "{workspaceRoot}"
      }
    },
    "docker-build": {
      "executor": "nx:run-commands",
      "dependsOn": ["docker-context"],
      "cache": true,
      "options": {
        "command": "docker build -t aitoearn-cloud-space --build-arg APP_NAME=aitoearn-cloud-space -f apps/aitoearn-cloud-space/Dockerfile ./dist/docker-context/aitoearn-cloud-space",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}

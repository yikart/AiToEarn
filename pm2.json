{
  "apps": [
    {
      "name": "aitoearn-web",
      "cwd": "standalone",
      "script": "node server.js",
      "instances": "1",
      "exec_mode": "cluster",
      "env": {
        "NODE_ENV": "development",
        "PORT": 7000
      },
      "env_production": {
        "NODE_ENV": "production",
        "PORT": 7000
      },
      "log_date_format": "YYYY-MM-DD_HH:mm Z",
      "merge_logs": true
    }
  ]
}

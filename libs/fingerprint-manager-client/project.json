{
  "name": "fingerprint-manager-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/fingerprint-manager-client/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/fingerprint-manager-client",
        "tsConfig": "libs/fingerprint-manager-client/tsconfig.lib.json",
        "packageJson": "libs/fingerprint-manager-client/package.json",
        "main": "libs/fingerprint-manager-client/src/index.ts",
        "assets": ["libs/fingerprint-manager-client/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/fingerprint-manager-client/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "coverageReporters": ["text"]
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "fix": true
      }
    }
  }
}

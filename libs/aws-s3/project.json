{
  "name": "aws-s3",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/aws-s3/src",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": [
        "dist/{projectRoot}"
      ],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/aws-s3",
        "tsConfig": "libs/aws-s3/tsconfig.lib.json",
        "packageJson": "libs/aws-s3/package.json",
        "main": "libs/aws-s3/src/index.ts",
        "assets": [
          "libs/aws-s3/*.md"
        ]
      }
    },
    "nx-release-publish": {
      "dependsOn": ["build"],
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "fix": true
      }
    }
  }
}

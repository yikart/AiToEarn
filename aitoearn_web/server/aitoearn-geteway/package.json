  "scripts": {
    "prepare": "mkdir -p .git/hooks && echo \"echo '{\\\"*.ts\\\":[\\\"eslint --fix\\\"]}' | npx lint-staged -c -\" > .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev:ol": "cross-env NATS_URL=nats://47.239.111.43:4222 NATS_PREFIX=dev nest start --watch",
    "dev": "nest start --watch -- -c config/dev.config.js",
    "dev:local": "nest start --watch -- -c config/local.config.js",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
